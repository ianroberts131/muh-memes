<%= provide(:title, @user.name) %>
<div class="row meme-creation">
  <%= render 'shared/meme_form' %>
</div>
<div class="row">
  <div class="col-md-8 col-md-offset-2 meme-thumbnails">
    <% if @user.memes.any? %>
      <h3>Muh Saved Memes (<%= @user.memes.count %>)</h3>
      <ul class="memes">
        <%= render @memes %>
      </ul>
      <%= will_paginate @memes %>
    <% end %>
  </div>
</div>
